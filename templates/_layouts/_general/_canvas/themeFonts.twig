{% if design.theme.value %}
    {% if design.theme.value == 'app-showcase' or design.theme.value == 'corporate' or design.theme.value == 'corporate-3' or 
        design.theme.value == 'corporate-4' or design.theme.value == 'onepage-3' or design.theme.value == 'portfolio-3' %}
        <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/inter.css">
        <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/playfair-display.css">
    {% endif %}

    {% if design.theme.value == 'coworking' or design.theme.value == 'photographer' or design.theme.value == 'photographer-dark' or 
        design.theme.value == 'seo' %}
        <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/poppins.css">
    {% endif %}

    {% if design.theme.value == 'cleaner' or design.theme.value == 'freelancer' %}
        <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/heebo.css">
    {% endif %}

    {% if design.theme.value == 'landing-2' or design.theme.value == 'landing-2-dark' %}
        <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/inter.css">
        <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/dm-serif-display.css">
    {% endif %}
    
    {% switch design.theme.value %}
        {% case "app-landing" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/poppins.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/montserrat.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/merriweather.css">
        {% case "burger" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/dosis.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/open-sans.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/dancing-script.css">
        {% case "business" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/open-sans.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/poppins.css">
        {% case "cafe" %}            
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/playfair-display.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/lato.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/nothing-you-could-do.css">
        {% case "construction" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/inter.css">
        {% case "covid-care" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/ibm-plex-sans.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/ibm-plex-serif.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/playfair-display.css">
        {% case "crowdfunding" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/montserrat.css">
        {% case "finance" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/manrope.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/dm-serif-text.css">
        {% case "football-club" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/oswald.css">
        {% case "furniture" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/zilla-slab.css">
        {% case "home" %}
            <link rel="stylesheet" href="https://use.typekit.net/iby7crc.css">
        {% case "hostel" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/inter.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/pt-serif.css">
        {% case "hosting" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/istok-web.css">
        {% case "insurance" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/quattrocento.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/shadows-into-light.css">
        {% case "integro" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/manrope.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/pt-serif.css">
        {% case "kindergarten" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
        {% case "landing" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/inter.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
        {% case "medical" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/lato.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/montserrat.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/crete-round.css">
        {% case "pet" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/quicksand.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/chewy.css">
        {% case "podcasts" %}
            <link rel="stylesheet" href="https://use.typekit.net/aay8rzy.css">
        {% case "recipes" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/noto-sans.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/noto-serif.css">
        {% case "restaurant" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/open-sans.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/poppins.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/playfair-display.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/cookie.css">
        {% case "saas" %}
            <link rel="stylesheet" href="https://use.typekit.net/vgf0rfn.css">
        {% case "saas-2" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/dm-sans.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/cookie.css">
        {% case "spa" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/lato.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/arimo.css"> 
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/playfair-display.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/cookie.css">
        {% case "speaker" %}
            <link rel="stylesheet" href="https://use.typekit.net/gmv6nzn.css">
        {% case "therapy" %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/prata.css">
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
        {% case "wine" %}
            <link rel="stylesheet" href="https://use.typekit.net/hky5sxh.css">
    
        {% default %}
            <link rel="stylesheet" href="{{ siteUrl }}template/css/fonts/roboto.css">
    {% endswitch %}

{% endif %}