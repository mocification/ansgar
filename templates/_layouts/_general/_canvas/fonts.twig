{% set fonts = [] %}

{% if design.fontFamilyPrimary.value %}
    {% set fonts = fonts|merge([design.fontFamilyPrimary.value]) %}
{% endif %}

{% if design.fontFamilySecondary.value %}
    {% if fonts|length %}
        {% if design.fontFamilySecondary.value not in fonts %}
            {% set fonts = fonts|merge([design.fontFamilySecondary.value]) %}
        {% endif %}
    {% else %}
        {% set fonts = fonts|merge([design.fontFamilySecondary.value]) %}
    {% endif %}
{% endif %}

{% if design.fontFamilyBody.value %}
    {% if fonts|length %}
        {% if design.fontFamilyBody.value not in fonts %}
            {% set fonts = fonts|merge([design.fontFamilyBody.value]) %}
        {% endif %}
    {% else %}
        {% set fonts = fonts|merge([design.fontFamilyBody.value]) %}
    {% endif %}
{% endif %}

